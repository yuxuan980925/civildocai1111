{
  "version": 2,
  "functions": {},
  "routes": [
    { "src": "^/demo$", "dest": "/public/demo.html" },
    { "src": "^/partner$", "dest": "/public/partner-demo.html" },
    { "src": "^/partner-demo$", "dest": "/public/partner-demo.html" },
    { "src": "^/deploy$", "dest": "/public/deploy.html" },
    { "src": "^/(.*)$", "dest": "/public/$1" },
    { "src": "^/$", "dest": "/public/index.html" }
  ],
  "headers": [
    {
      "source": "/public/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=3600"
        }
      ]
    }
  ]
}